<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DelegacionMAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:acceso="clr-namespace:DelegacionMAUI.Acceso"
    xmlns:catalogo="clr-namespace:DelegacionMAUI.Catalogo"
    Shell.FlyoutBehavior="Flyout">

    <ShellItem Route="Login">
        <ShellContent ContentTemplate="{DataTemplate acceso:Login}" />
    </ShellItem>

    <ShellItem Route="HomePages">
        <ShellContent ContentTemplate="{DataTemplate catalogo:HomePages}" />
        
    </ShellItem>

</Shell>
